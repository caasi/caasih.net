{
  "name": "caasih.net",
  "version": "0.1.1",
  "description": "my own graveyard",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently --kill-others npm:dev npm:re:watch",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack-dev-server",
    "clean": "rimraf ./dist/*bundle.js ./dist/index.html ./dist/404.html ./dist/nostr.json",
    "clean:manifest": "rimraf ./dist/*.manifest.js",
    "lint": "eslint src",
    "re:build": "bsb -make-world -clean-world",
    "re:watch": "bsb -make-world -clean-world -w",
    "build:will": "pug --out ./dist ./src/continuity.jade",
    "build:webpack": "npm run re:build && webpack --config ./build.webpack.config.js",
    "build:snapshot": "react-snapshot --build-dir ./dist",
    "build:data": "cp ./json/nostr.json ./dist",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build:webpack && npm run build:snapshot && npm run build:data && npm run clean:manifest",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "codecov": "jest && codecov",
    "prettier": "npx prettier --single-quote --no-semi --trailing-comma es5 --arrow-parens always --write '*.js' 'config/**/*.js'",
    "update:browserslist": "npx browserslist@latest --update-db"
  },
  "keywords": [
    "react",
    "personal"
  ],
  "author": "caasi Huang",
  "license": "MIT",
  "devDependencies": {
    "@mdx-js/loader": "^1.0.0-rc.4",
    "@swc/core": "^1.2.80",
    "@swc/jest": "^0.2.2",
    "autoprefixer": "9",
    "bs-let": "^0.1.16",
    "bs-platform": "^8.2.0",
    "concurrently": "^5.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5",
    "eslint-plugin-react": "7",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "gentype": "^3.21.0",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "3",
    "identity-obj-proxy": "^3.0.0",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "jest": "24",
    "json-loader": "^0.5.7",
    "jstransformer-markdown-it": "^2.0.0",
    "postcss": "7",
    "postcss-loader": "3",
    "postcss-scss": "2",
    "precss": "4",
    "prettier": "1.16.4",
    "pug": "3",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "2",
    "purescript": "^0.12.0",
    "purescript-psa": "^0.7.2",
    "purs-loader": "3",
    "raf": "^3.4.0",
    "react-hot-loader": "4",
    "react-snapshot": "^1.3.0",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.23.1",
    "swc-loader": "^0.1.14",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@caasi/hooks": "^1.7.3",
    "@caasi/then": "^1.0.2",
    "@rescript/react": "^0.10.1",
    "axios": "^0.21.1",
    "bs-webapi": "^0.19.1",
    "classnames": "^2.2.6",
    "jimp": "^0.6.1",
    "konva": "^7.2.0",
    "moment": "^2.19.3",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "raw-loader": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^5.2.0",
    "react-konva": "^17.0.2-5",
    "react-markdown": "^4.2.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^9.0.1",
    "webvr-polyfill": "^0.10.11"
  },
  "reactSnapshot": {
    "snapshotDelay": 300,
    "include": [
      "/404"
    ]
  }
}
